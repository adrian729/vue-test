        "@fortawesome/vue-fontawesome": "^3.0.6",
        "axios": "^1.6.8",
        "vue": "^3.4.21",
        "vue-router": "^4.3.0"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^5.0.4",
        "autoprefixer": "^10.4.18",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-vue": "^9.23.0",
        "node-sass": "^9.0.0",
        "postcss": "^8.4.36",
        "prettier": "3.2.5",
